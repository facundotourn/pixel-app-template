{
  "name": "braindw-io",
  "vendor": "braindw",
  "version": "0.0.6-beta.0",
  "title": "BrainDW IO",
  "description": "Pixel app for BrainDW clients",
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true,
    "type": "free",
    "availableCountries": [
      "*"
    ]
  },
  "defaultLocale": "es-AR",
  "builders": {
    "react": "3.x",
    "store": "0.x",
    "pixel": "0.x",
    "docs": "0.x",
    "scripts": "0.x"
  },
  "dependencies": {
    "vtex.native-types": "0.x",
    "vtex.product-summary": "2.x",
    "vtex.pixel-interfaces": "1.x",
    "vtex.device-detector": "0.x",
    "vtex.search-graphql": "0.x",
    "vtex.product-context": "0.x",
    "vtex.slider": "0.x",
    "vtex.pixel-manager": "1.x",
    "vtex.store-icons": "0.x",
    "vtex.css-handles": "0.x",
    "vtex.product-list-context": "0.x"
  },
  "settingsSchema": {
    "title": "braindw-io",
    "type": "object",
    "properties": {
      "clientKey": {
        "title": "Client KEY",
        "description": "Enter the client key provided by BrainDW",
        "type": "string",
        "default": "default-client-key"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
